{
  "name": "resume",
  "version": "0.0.0",
  "private": true,
  "scripts": {
   "start": "node ./bin/www",
    "server": "nodemon ./bin/www",
    "client": "yarn dev --prefix client",
    "client-install": "yarn install --prefix client",
    "dev": "concurrently \"npm run server\" \"npm run client\"",
    "heroku-postbuild": "NPM_CONFIG_PRODUCTION=false npm install --prefix client && npm run build --prefix client"
  },
  "engines": {
    "node": "14.x"
  },
  "dependencies": {
    "bcrypt": "^5.0.1",
    "concurrently": "^6.2.1",
    "config": "^3.3.6",
    "consola": "^2.15.3",
    "cookie-parser": "~1.4.4",
    "debug": "~2.6.9",
    "dotenv": "^10.0.0",
    "express": "~4.16.1",
    "joi": "^17.4.2",
    "jsonwebtoken": "^8.5.1",
    "mongoose": "^5.13.7",
    "morgan": "~1.9.1"
  },
  "devDependencies": {
    "nodemon": "^2.0.12"
  }
}
